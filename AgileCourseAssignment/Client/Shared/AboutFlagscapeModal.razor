
<h1>Test</h1>
<table border="1">
    <tr>
        <th>Question Number</th>
        <th>Your Answer</th>
        <th>Correct Country</th>
    </tr>
    @for (var i = 0; i < UserAnswers.Count; i++)
    {
        <tr>
            <td>@(i + 1)</td>
            <td>@UserAnswers[i]</td>
        </tr>
    }
</table>

@code {
    [Parameter]
    public List<string> UserAnswers { get; set; }
}